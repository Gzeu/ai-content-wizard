{
  "name": "ai-content-wizard",
  "version": "1.0.0",
  "description": "A powerful AI-powered content generation tool using Groq's API",
  "main": "src/ai-provider.js",
  "bin": {
    "ai-wizard": "./src/cli.js"
  },
  "scripts": {
    "start": "node src/cli.js",
    "dev": "node --watch src/cli.js",
    "test": "node test-app.js",
    "test:debug": "DEBUG_GROQ=true node test-app.js",
    "lint": "eslint src/**.js",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "build": "pkg . --out-path dist"
  },
  "keywords": [
    "ai",
    "groq",
    "llm",
    "content-generation",
    "cli",
    "llama3",
    "mixtral",
    "ai-tools"
  ],
  "author": "AI Content Wizard Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-content-wizard.git"
  },
  "homepage": "https://github.com/yourusername/ai-content-wizard#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.0",
    "ora": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "jest": "^29.7.0",
    "pkg": "^5.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pkg": {
    "outputPath": "dist",
    "targets": ["node18-linux-x64", "node18-macos-x64", "node18-win-x64"],
    "assets": [".env"]
  }
}